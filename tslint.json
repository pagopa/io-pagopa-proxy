{
  "defaultSeverity": "error",
  "extends": [
    "italia-tslint-rules/strong"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "no-submodule-imports": [
      true,
      "fp-ts",
      "io-ts",
      "italia-ts-commons",
      "italia-pagopa-commons"
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "generated/**/*"
    ]
  }
}